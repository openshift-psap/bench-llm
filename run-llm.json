{
  "tags": {
    "topology": "none"
  },
  "endpoints": [
    {
      "type": "remotehosts",
      "remotes": [
        {
          "engines": [
            {
              "role": "client",
              "ids": "1"
            }
          ],
          "config": {
            "host": "hostname",
            "settings": {
              "userenv": "fedora40",
              "osruntime": "podman",
              "podman-settings": {
                "shm-size": "10.00gb"
              },
              "cpu-partitioning": false
            }
          }
        }
      ]
    }
  ],
  "run-params": {
    "num-samples": 1,
    "max-sample-failures": 1,
    "test-order": "r"
  },
  "tool-params": [
    {
      "tool": "sysstat",
      "params": [
        {
          "arg": "subtools",
          "val": "mpstat,sar,iostat"
        },
        {
          "arg": "interval",
          "val": "15"
        }
      ]
    },
    {
      "tool": "procstat",
      "params": [
        {
          "arg": "interval",
          "val": "15"
        }
      ]
    }
  ],
  "benchmarks": [
    {
      "name": "llm",
      "ids": "1",
      "mv-params": {
        "sets": [
          {
            "params": []
          }
        ]
      }
    }
  ]
}
